<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="dialog" alignment="TOP_CENTER" minHeight="120.0" minWidth="550.0" onKeyPressed="#onKeyPressed" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.endoskop.view.DatePeriodViewController">
   <children>
      <Label id="title" text="За период">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER">
         <children>
            <Label id="medium-label" text="С">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="from" onKeyReleased="#keyReleasedOnDateTF" >
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </TextField>
            <Label id="medium-label" text="ПО">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="to" onKeyReleased="#keyReleasedOnDateTF"/>
         </children>
      </HBox>
   </children>
</VBox>
