<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox id="card" fx:id="root" alignment="CENTER" minWidth="700.0" onKeyPressed="#onKeyPressed" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.endoskop.view.PrintViewController">
   <children>
      <Label id="print-title" fx:id="tableName" text="Label" />
      <Label id="print-title" fx:id="period" text="Label" />
      <TableView id="card" fx:id="table" prefWidth="700.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn prefWidth="75.0" text="Column X" />
         </columns></TableView>
      <GridPane id="card" fx:id="footer">
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </children>
</VBox>
